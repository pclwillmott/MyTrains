<?xml version="1.0"?>
<cdi xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openlcb.org/schema/cdi/1/1/cdi.xsd">
<identification>
  <manufacturer>%%MANUFACTURER%%</manufacturer>
  <model>%%MODEL%%</model>
  <hardwareVersion>%%HARDWARE_VERSION%%</hardwareVersion>
  <softwareVersion>%%SOFTWARE_VERSION%%</softwareVersion>
</identification>

<acdi/>
<segment space='251' origin='1'>
<name>Node ID</name>
<string size='63'>
<name>User Name</name>
<description>This name will appear in network browsers for this device.</description>
</string>
<string size='64'>
<name>User Description</name>
<description>This description will appear in network browsers for this device.</description>
</string>
</segment>

<segment space='253' origin='0'>
<name>Configuration</name>
<group>
<name>Serial Port</name>
<description>Configures the serial port</description>
<string size='256'>
<name>Port</name>
<map>
  %%PORTS%%
</map>
</string>

<int size='1'>
<name>Baud Rate</name>
<map>
  <relation><property>0</property><value>9600</value></relation>
  <relation><property>1</property><value>19200</value></relation>
  <relation><property>2</property><value>28800</value></relation>
  <relation><property>3</property><value>38400</value></relation>
  <relation><property>4</property><value>57600</value></relation>
  <relation><property>5</property><value>76800</value></relation>
  <relation><property>6</property><value>115200</value></relation>
  <relation><property>7</property><value>125000</value></relation>
  <relation><property>8</property><value>230400</value></relation>
  <relation><property>9</property><value>460800</value></relation>
  <relation><property>10</property><value>576000</value></relation>
  <relation><property>11</property><value>921600</value></relation>
</map>
</int>

<int size='1'>
<name>Parity</name>
<map>
  <relation><property>0</property><value>None</value></relation>
  <relation><property>1</property><value>Even</value></relation>
  <relation><property>2</property><value>Odd</value></relation>
</map>
</int>

<int size='1'>
<name>Flow Control</name>
<map>
  <relation><property>0</property><value>No Flow Control</value></relation>
  <relation><property>1</property><value>RTS/CTS</value></relation>
</map>
</int>
</group>
</segment>

</cdi>




